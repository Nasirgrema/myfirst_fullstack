generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Track {
  id        String   @id @default(uuid())
  title     String
  artist    String
  filePath  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

